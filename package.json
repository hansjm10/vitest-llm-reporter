{
  "name": "vitest-llm-reporter",
  "version": "0.1.0",
  "type": "module",
  "description": "A Vitest reporter optimized for LLM parsing with structured, concise output",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vitest",
    "reporter",
    "llm",
    "ai",
    "testing",
    "structured-output"
  ],
  "author": "Jordan Hans",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordan-hans/vitest-llm-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/jordan-hans/vitest-llm-reporter/issues"
  },
  "homepage": "https://github.com/jordan-hans/vitest-llm-reporter#readme",
  "engines": {
    "node": ">=17.0.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
